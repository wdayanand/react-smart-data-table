{
  "name": "react-smart-data-table",
  "version": "0.6.7",
  "description": "A smart data table component for React.js meant to be configuration free",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --config webpack.config.js",
    "dev": "npm run build-dev && npm run test-dev",
    "prd": "npm run build && npm run test",
    "build": "NODE_ENV=production webpack --mode production --config webpack.config.js",
    "build-dev": "NODE_ENV=development webpack --mode development --config webpack.config.js",
    "test": "NODE_ENV=production webpack --mode production --config webpack.test.js",
    "test-dev": "NODE_ENV=development webpack --mode development --config webpack.test.js"
  },
  "main": "dist/react-smart-data-table.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joaocarmo/react-smart-data-table.git"
  },
  "keywords": [
    "react",
    "simple",
    "smart",
    "data",
    "table",
    "configuration"
  ],
  "author": "Joao Carmo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joaocarmo/react-smart-data-table/issues"
  },
  "homepage": "https://github.com/joaocarmo/react-smart-data-table#readme",
  "dependencies": {
    "camelcase": "^5.0.0",
    "escape-string-regexp": "^1.0.5",
    "flat": "^4.1.0",
    "linkifyjs": "^2.1.7",
    "memoize-one": "^5.0.0",
    "react-custom-scrollbars": "^4.2.1",
    "segmentize": "^0.4.1",
    "semantic-ui-react": "^0.84.0",
    "url-loader": "^1.1.2"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.2.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "core-js": "^2.6.1",
    "css-loader": "^2.0.2",
    "eslint": "^5.11.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "faker": "^4.1.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.28.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.1.5"
  }
}
